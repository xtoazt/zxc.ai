{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/generate-video.js": {
      "maxDuration": 300
    },
    "api/generate-video-with-image.js": {
      "maxDuration": 300
    },
    "api/enhance-prompt.js": {
      "maxDuration": 30
    },
    "api/ai-suggestions.js": {
      "maxDuration": 30
    },
    "api/generate-transition-video.js": {
      "maxDuration": 300
    },
    "api/ai-chat.js": {
      "maxDuration": 30
    },
    "api/generate-video-vider.js": {
      "maxDuration": 300
    },
    "api/chatglm-chat.js": {
      "maxDuration": 60
    },
    "api/generate-video-huggingface.js": {
      "maxDuration": 300
    },
    "api/generate-video-replicate.js": {
      "maxDuration": 300
    },
    "api/generate-video-pexels.js": {
      "maxDuration": 60
    },
    "api/generate-video-gemini.js": {
      "maxDuration": 60
    },
    "api/v1/chat/completions.js": {
      "maxDuration": 300
    },
    "api/v1/models.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/v1/chat/completions",
      "destination": "/api/v1/chat/completions"
    },
    {
      "source": "/v1/models",
      "destination": "/api/v1/models"
    }
  ]
}
