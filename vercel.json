{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/generate-video.js": {
      "maxDuration": 300
    },
    "api/generate-video-with-image.js": {
      "maxDuration": 300
    },
    "api/generate-transition-video.js": {
      "maxDuration": 300
    },
    "api/generate-video-vider.js": {
      "maxDuration": 300
    },
    "api/generate-video-huggingface.js": {
      "maxDuration": 300
    },
    "api/generate-video-replicate.js": {
      "maxDuration": 300
    },
    "api/generate-video-pexels.js": {
      "maxDuration": 60
    },
    "api/generate-video-gemini.js": {
      "maxDuration": 60
    },
    "api/v1.js": {
      "maxDuration": 300
    },
    "api/api-router.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/api-router"
    },
    {
      "source": "/api/options",
      "destination": "/api/api-router"
    },
    {
      "source": "/api/enhance-prompt",
      "destination": "/api/api-router"
    },
    {
      "source": "/api/ai-suggestions",
      "destination": "/api/api-router"
    },
    {
      "source": "/api/ai-chat",
      "destination": "/api/api-router"
    },
    {
      "source": "/api/chatglm-chat",
      "destination": "/api/api-router"
    },
    {
      "source": "/v1/chat/completions",
      "destination": "/api/v1"
    },
    {
      "source": "/v1/models",
      "destination": "/api/v1"
    }
  ]
}
